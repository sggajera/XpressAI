{
    "name": "express-ai",
    "version": "1.0.0",
    "scripts": {
        "start": "node server/src/index.js",
        "dev": "nodemon server/src/index.js",
        "debug": "node --inspect server/src/index.js",
        "debug:break": "node --inspect-brk server/src/index.js",
        "client": "cd client && npm start",
        "dev:full": "concurrently \"npm run dev\" \"npm run client\"",
        "build": "cd client && npm run build"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "mongoose": "^6.10.0",
        "openai": "^4.86.1",
        "twitter-api-v2": "^1.20.2"
    },
    "devDependencies": {
        "concurrently": "^8.0.1",
        "nodemon": "^2.0.22"
    }
}
